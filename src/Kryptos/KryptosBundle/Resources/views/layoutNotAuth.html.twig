    {% extends "::base.html.twig" %}

{% block head %}
	<meta charset="UTF-8" />
    <title>{% block title %}Kryptos{% endblock %}</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-Ua-Compatible" content="IE=edge,chrome=1" />
	<meta name="language" content="en" />
	<link rel="stylesheet" type="text/css" href="/bundles/kryptoskryptos/css/bootstrap/bootstrap.css" />
	<link rel="stylesheet" type="text/css" href="/bundles/kryptoskryptos/css/main.css" />
	<!-- <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" /> -->
{% endblock %}

{% block body %}
<div class="row-fluid">
	<div class="span3"></div>
	<div class="span6">
		{% block content %}{% endblock %}
	</div>
	<div class="span3"></div>
</div>
{% endblock %}

{% block javascripts %}
<script src="/bundles/kryptoskryptos/js/jquery-1.9.1.min.js"></script>

<script type="text/javascript" src="/bundles/kryptoskryptos/js/bootstrap/bootstrap.js"></script>
<script type="text/javascript" src="/bundles/kryptoskryptos/js/bootstrap/bootstrap-tooltip.js"></script>
<script type="text/javascript">
$(document).ready(function() {
	$("input[rel=tooltip]").tooltip({ trigger: 'hover' ,animation: 'true' , placement:'right'});
});
</script>
{% endblock %}