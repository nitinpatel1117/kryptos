    {% extends "::base.html.twig" %}

{% block head %}
	<meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-Ua-Compatible" content="IE=edge,chrome=1" />
	<meta name="language" content="en" />
	<link rel="stylesheet" href="/bundles/kryptoskryptos/css/main.css" type="text/css" />
	
	<!-- <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" /> -->
{% endblock %}

{% block body %}
<section class="centraliser">
	<header>
		{% render 'KryptosKryptosBundle:Default:authenicationBar' %}
	</header>
	
	<section class="contentWrapper">
		{% block content %}{% endblock %}
	</section>
	
	<footer>
  		<p>(c) 2013 Kryptos</p>
	</footer>
</section>

<script src="/bundles/kryptoskryptos/js/jquery-1.9.1.min.js"></script>
<script src="/bundles/kryptoskryptos/js/purchaseConversions.js"></script>
<script src="/bundles/kryptoskryptos/js/fileStatus.js"></script>
{% endblock %}